{
  "name": "angular-pouchdb",
  "version": "0.1.2",
  "authors": [
    "Wilfred Springer <wilfredspringer@gmail.com>"
  ],
  "description": "Angular wrapper for PouchDB, making sure that callbacks are called within $rootScope.$apply(), and using $q promises instead of callbacks.",
  "main": "angular-pouchdb.js",
  "keywords": [
    "angularjs",
    "pouchdb"
  ],
  "license": "MIT",
  "homepage": "https://github.com/wspringer/angular-pouchdb",
  "ignore": [
    "**/.*",
    "test",
    "tests",
    "external",
    "karma.conf.js"
  ],
  "dependencies": {
    "angular": "~1.2.3",
    "pouchdb-nightly.min.js": "http://download.pouchdb.com/pouchdb-nightly.min.js"
  },
  "devDependencies": {
    "angular-mocks": "~1.2.3"
  }
}
